<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id="empId" class="com._02.DI.EmployeeConstructorInjection">
    <constructor-arg name="name" value="Zuleyha"/>
    <constructor-arg name="surname" value="Kaya"/>
    <constructor-arg name="salary" value="10000"/>
    <constructor-arg name="birthYear" value="1993"/>
    <constructor-arg value="K"/>
</bean>
</beans>